{
    "name": "brighty/cilog",
    "description": "A simple log library for Internal use.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Brighty\\Cilog\\": "src/",
            "Brighty\\Cilog\\Providers\\": "src/app/Providers/",
            "Brighty\\Cilog\\Console\\": "src/app/Console/"
        }
    },
    "authors": [
        {
            "name": "Egi Aprilianto",
            "email": "contact@asln.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^9.0",
        "illuminate/console": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brighty\\Cilog\\Providers\\CilogServiceProvider"
            ]
        }
    }
}
